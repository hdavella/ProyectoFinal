<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ElectroSoft - Admin</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css"
        integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300;400;700&display=swap" rel="stylesheet" />
    <link rel="stylesheet" href="/stylesheets/normalize.css" />
    <link rel="stylesheet" href="/stylesheets/transportes.css" />
    <script>
        let redirect_Page = (ele) => {
            ele.disabled = true;
            let iCnt = 2;
            let iTimerId = setInterval(function () {
                document.getElementById('msg').innerHTML = 'Ser√° redireccionado en ' + iCnt + ' segundos';

                iCnt--;

                if (iCnt === 0) {
                    window.location.href = '/admin/trabajos';
                    clearInterval(iTimerId);
                }
            }, 1000);
        }
    </script>
</head>

<body onload="redirect_Page(this)">
    <p style="color: #777;font-size: 20px;" >Alta Correcta!</p>
    <p style="color: #777;font-size: 10px;" id='msg'></p>
</body>